.u-demo-block__content {
  width: 80%;
  height: 33%;
  line-height: 100rpx;
  position: absolute;
  top: 200rpx;
  background-color: #ffffff;
  display: grid;
  place-items: center;
  left: 10%;
  border-bottom-left-radius: 40px;
  border-bottom-right-radius: 40px;
  overflow: hidden;
  background: linear-gradient(135deg, transparent 25px, #ffffff 0) top left, linear-gradient(-135deg, transparent 25px, #ffffff 0) top right;
  background-size: 80% 100%;
  background-repeat: no-repeat;
  border: 1px solid #ffd425;
}
.u-demo-block_top {
  position: absolute;
  top: -100rpx;
  width: 100%;
  height: 500rpx;
  background: url(/static/loginbgi.jpg) no-repeat center;
}
.u-demo-block {
  background-color: #f7f7f7;
}
.input {
  background-color: #ffffff;
  border: 0px;
  color: #0097e3;
  position: relative;
  left: -15%;
}
.phone {
  position: relative;
  top: 15px;
  left: -38%;
  font-size: 10px;
}
.u-demo-block__bottom {
  position: absolute;
  top: 45%;
  width: 80%;
  height: 100px;
  display: grid;
  place-items: center;
  left: 10%;
}

